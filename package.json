    "highlight.js": "^11.11.1",
    "lifecycle-utils": "^2.0.0",
    "markdown-it": "^14.1.0",
    "node-llama-cpp": "^3.8.1",
    "pretty-ms": "^9.2.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "semver": "^7.7.1",
    "tailwindcss": "3.4.0"
